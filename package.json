{
  "name": "loopless",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "dev": "pnpm -r --parallel dev",
    "dev:server": "pnpm --filter server dev",
    "dev:web": "pnpm --filter web dev",
    "demo:twice": "pnpm --filter server demo:twice",
    "seed:tasks": "pnpm exec tsx scripts/seed_tasks.ts",
    "eval": "pnpm exec tsx scripts/run_eval.ts"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "pnpm@9.0.0"
}
