    {
      "description": "Current step updated to confirmation",
      "type": "jmespath",
      "expected_value": "confirmation",
      "query": "values_changed.\"root['points']['currentStep']\".new_value"
    },
    {
      "description": "Purchase history entry added with 10,000 points",
      "type": "jmespath",
      "expected_value": 10000,
      "query": "iterable_item_added.* | [?points==`10000`].points | [0]"
    },
    {
      "description": "Purchase history entry records 1,000 bonus points",
      "type": "jmespath",
      "expected_value": 1000,
      "query": "iterable_item_added.* | [?bonusPoints==`1000`].bonusPoints | [0]"
    }
  ],
