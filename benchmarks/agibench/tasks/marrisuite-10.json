{
  "id": "marrisuite-10",
  "goal": "Book a 5-night stay in Cancun for July 18-23, use points (and card if needed) fill in dummy information to complete the task (This is a test environment).",
  "website": {
    "id": "marrisuite",
    "name": "MarriSuite",
    "similarTo": "Marriott",
    "url": "https://real-marrisuite.vercel.app/"
  },
  "difficulty": "medium",
  "challengeType": "action",
  "possible": true,
  "evals": [
    {
      "description": "Destination contains Cancun, Mexico",
      "type": "jmespath",
      "expected_value": true,
      "query": "type_changes.\"root['booking']['currentBooking']\".new_value.destination | contains(@, 'Cancun')"
    },
    {
      "description": "Check-in date set to July 18th, 2024",
      "type": "jmespath",
      "expected_value": "2024-07-18T08:00:00.000Z",
      "query": "type_changes.\"root['booking']['currentBooking']\".new_value.dates.startDate"
    },
    {
      "description": "Check-out date set to July 23rd, 2024 (5-night stay)",
      "type": "jmespath",
      "expected_value": "2024-07-23T08:00:00.000Z",
      "query": "type_changes.\"root['booking']['currentBooking']\".new_value.dates.endDate"
    },
    {
      "description": "Payment method includes points (points and card if needed)",
      "type": "jmespath",
      "expected_value": true,
      "query": "type_changes.\"root['booking']['currentBooking']\".new_value.paymentInfo.pointsUsed | @ > `0`"
    }
  ],
  "points": 1,
  "config": {}
}